{
    "author": "Lewis Van Winkle",
    "name": "tulind",
    "version": "0.9.21",
    "description": "Tulip Indicators Wrapper Providing Over 100 Technical Analysis Functions",
    "main": "index.js",
    "gypfile": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/jazarja/tulipnode.git"
    },
    "keywords": [
        "tulip",
        "charts",
        "charting",
        "indicators",
        "oscillator",
        "overlay",
        "technical",
        "analysis",
        "finance",
        "trading",
        "markets"
    ],
    "license": "LGPL-3.0",
    "dependencies": {
        "bindings": "^1.5.0",
        "nan": "^2.16.0",
        "@mapbox/node-pre-gyp": "^1.0.5",
        "minctest": "0.0.x"
    },
    "devDependencies": {
        "aws-sdk": "2.x",
        "chai": "4.4.1",
        "mocha": "10.4.0"
    },
    "scripts": {
        "install": "node-pre-gyp install --fallback-to-build",
        "package": "node-pre-gyp package",
        "test": "mocha"
    },
    "binary": {
        "module_name": "tulind",
        "module_path": "./lib/binding/{configuration}/{node_abi}-{platform}-{arch}/",
        "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
        "host": "https://github.com/jazarja/tulipnode/releases/download/",
        "remote_path": "{version}"
    }
}